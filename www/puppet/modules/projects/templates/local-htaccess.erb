#php_value error_log /var/www/projects/logs/php.errors


<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Default document
  RewriteRule ^(/)?$ /fe/index.php

  # Application
  #    RewriteRule ^a/pr/(.*)$ a/pr/ [L]
  RewriteRule ^a$ app.php
  RewriteRule ^a/(.*)$ app.php [QSA,L]


  # API
  RewriteRule ^api/(.*)$ app.php [QSA,L]

  # Application resources
  RewriteRule ^js/(.*)$ app.php [QSA,L]
  RewriteRule ^css/(.*).css$ app.php [QSA,L]

  # Application profiler
  RewriteRule ^_wdt/(.*)$ app.php [QSA,L]
  RewriteRule ^_profiler/(.*)$ app.php [QSA,L]



  # Redirect any non-existing file, directory and symbolic link to frontend
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-l
  RewriteRule ^(.*)$ /fe/$1 [QSA,L]
</IfModule>

